{
  "name": "beep-intercom-chatbot",
  "version": "1.0.0",
  "description": "StoreHub BEEP Intercom Chatbot for order status queries",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:worker": "node worker.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:worker\"",
    "dev": "nodemon app.js",
    "dev:worker": "nodemon worker.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:worker\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/integration",
    "test:webhook": "node scripts/test-webhook.js",
    "test:e2e": "jest tests/e2e --runInBand",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "axios": "^1.6.0",
    "bull": "^4.11.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "opossum": "^8.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nock": "^13.5.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "chatbot",
    "intercom",
    "delivery",
    "storehub",
    "beep"
  ],
  "author": "StoreHub Team",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/tests/**"
    ]
  }
}